<%@ page import="java.util.Date" %>
<%@ page import="java.text.SimpleDateFormat" %>

<%
        Date now = new Date();
        SimpleDateFormat sf = new SimpleDateFormat("yyyy");
%>

<input type="text" id="startDate" name="startDate" value="<%=sf.format(now));%>" &nbsp;&nbsp--%>


