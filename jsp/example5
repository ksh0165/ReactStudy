<% 
	request.setCharacterEncoding("UTF-8");
	Enumeration<String> para = request.getParameterNames();
	while(para.hasMoreElements()){
		String pName = para.nextElement();
		out.println(pName+":");
		String pValue = request.getParameter(pName);
		out.println(pValue+"<br>");
	}
%>

