select max(cnt) from (select DATE_FORMAT(REPORTED_TIME,'%Y%m%d') as date ,count(*) as cnt from 테이블명 where REPORTED_TIME like '2021-03%' group by DATE_FORMAT(REPORTED_TIME,'%Y%m%d')) as result;
